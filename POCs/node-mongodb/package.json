{
  "name": "nodejs-express-typescript",
  "version": "1.0.0",
  "description": "CKL - Express Typescript Boilerplate",
  "main": "src/app.ts",
  "scripts": {
    "start": "NODE_PATH=dist/ node -r ts-node/register dist/app.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --trace-warnings --inspect --respawn --transpile-only --ignore-watch node_modules src/app.ts",
    "dev:watch": "nodemon -r tsconfig-paths/register --files src/app.ts",
    "build": "tsc",
    "test": "NODE_ENV=test jest --config=./src/jest.config.js",
    "test:no-cache": "NODE_ENV=test jest --config=./src/jest.config.js --no-cache",
    "test:coverage": "NODE_ENV=test jest --config=./src/jest.config.js --coverage",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs/",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/database",
    "migrate:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/database migration:generate",
    "migrate:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/database migration:run",
    "migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/database migration:revert",
    "lint": "eslint src/",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,json,md}"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.8",
    "@types/glob": "^8.0.0",
    "@types/jest": "~27.5.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^14.11.10",
    "@types/prompt": "^1.1.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.30.7",
    "apidoc": "^0.54.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^43.0.2",
    "jest": "~28.0.3",
    "prettier": "^2.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "~28.0.2",
    "ts-node": "~10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.6.3"
  },
  "dependencies": {
    "@sentry/node": "^5.12.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "graphql": "~15.8.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.4",
    "pino": "^8.9.0",
    "pino-pretty": "^9.1.1",
    "prompt": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.0",
    "socket.io": "^4.5.2",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "zod": "^3.20.2",
    "zod-to-json-schema": "^3.20.2"
  }
}
